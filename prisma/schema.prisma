generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ttvUser_172197890 {
  ID        Int       @id @default(autoincrement())
  SenderID  String?   @db.VarChar(10)
  Name      String    @default("No name provided") @db.VarChar(30)
  Message   String?   @db.Text
  Emotes    String?   @db.Text
  Color     String?   @db.VarChar(7)
  Badges    String?   @db.Text
  Timestamp DateTime? @default(now()) @db.Timestamp(0)
  isDeleted Int       @default(0) @db.TinyInt

  @@index([SenderID], map: "SenderID")
}

model Channels {
  ChannelID  String    @id @db.VarChar(20)
  Name       String    @db.VarChar(30)
  Timestamp  DateTime? @default(now()) @db.Timestamp(0)
  Availiable Int       @default(1) @db.TinyInt
}

